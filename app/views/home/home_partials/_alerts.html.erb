<% if model.errors.any? %>
  <% model.errors.keys.each do |key| %>
    <div class="alert alert-danger alert-dismissible">
      <button type="button" class="close" data-dismiss="alert">&times;</button>
      <strong><%= key.to_s.humanize %></strong> <%= model.errors[key].first %>
    </div>
  <% end %>
<% end %>
